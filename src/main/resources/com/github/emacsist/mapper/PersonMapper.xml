<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.emacsist.mapper.PersonMapper">
    <select id="selectOne" resultType="com.github.emacsist.pojo.Person">
        select * from person where id = #{id}
    </select>
</mapper>